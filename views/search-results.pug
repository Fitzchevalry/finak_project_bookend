ul
  each user in users
    li
      .friends_profile_pic
        img(src=user.profile_pic, width='100', height='100')
      h3 #{user.firstname} #{user.lastname}
      button(type="button", id=`${user.member_id}_request_button`) Envoyer une invitation

      

  script(src="/js/friends-script.js")
  script.
    document.getElementById("search_find").addEventListener("click", function() {
      window.location.href = "http://localhost:3000/search-friends-results?searchQuery=#{searchQuery}";
    });
